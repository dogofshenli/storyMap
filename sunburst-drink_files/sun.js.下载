emphasis: {
  focus: 'ancestor'
},
levels: [
  {},
  {
    r0: '15%',
    r: '35%',
    itemStyle: {
      borderWidth: 2
    },
    label: {
      rotate: 'tangential'
    }
  },
  {
    r0: '35%',
    r: '70%',
    label: {
      align: 'right'
    }
  },
  {
    r0: '70%',
    r: '72%',
    label: {
      position: 'outside',
      padding: 3,
      silent: false
    },
    itemStyle: {
      borderWidth: 3
    }
  }
]
}
};
if (option && typeof option === 'object') {
  myChart.setOption(option);
}
window.addEventListener('resize', myChart.resize);
